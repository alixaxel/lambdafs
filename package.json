{
  "name": "lambdafs",
  "private": false,
  "version": "0.0.1",
  "author": {
    "name": "Alix Axel"
  },
  "license": "MIT",
  "description": "Efficient (de)compression package for AWS Lambda",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "brotli": "bin/brotli.js"
  },
  "files": [
    "bin",
    "build"
  ],
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json && cpx 'source/iltorb/**' build/iltorb",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "tar-fs": "^2.0.0"
  },
  "optionalDependencies": {
    "iltorb": "^2.4.3"
  },
  "devDependencies": {
    "@types/node": "12.7.5",
    "@types/tar-fs": "^1.16.1",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "bundledDependencies": [
    "tar-fs"
  ],
  "bugs": {
    "url": "https://github.com/alixaxel/lambdafs/issues"
  },
  "homepage": "https://github.com/alixaxel/lambdafs",
  "repository": {
    "type": "git",
    "url": "git://github.com/alixaxel/lambdafs.git"
  },
  "keywords": [
    "aws",
    "brotli",
    "compression",
    "decompression",
    "gzip",
    "lambda",
    "serverless",
    "tarball"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
