{
  "name": "lambdafs",
  "private": false,
  "version": "2.1.1",
  "author": {
    "name": "Alix Axel"
  },
  "license": "MIT",
  "description": "Efficient (de)compression package for AWS Lambda",
  "bin": {
    "lambdafs": "bin/brotli.js"
  },
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "files": [
    "bin",
    "build"
  ],
  "engines": {
    "node": ">= 10.16"
  },
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "tar-fs": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^12.20.38",
    "@types/tar-fs": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "bugs": {
    "url": "https://github.com/alixaxel/lambdafs/issues"
  },
  "homepage": "https://github.com/alixaxel/lambdafs",
  "repository": {
    "type": "git",
    "url": "git://github.com/alixaxel/lambdafs.git"
  },
  "keywords": [
    "aws",
    "brotli",
    "compression",
    "decompression",
    "gzip",
    "lambda",
    "serverless",
    "tarball"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "bundleDependencies": [
    "tar-fs"
  ]
}
